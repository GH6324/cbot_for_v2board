[Database]
# 数据库配置
host = 127.0.0.1
port = 3306
user = user
pwd = pwd
db = dbname

[Telegram]
# 机器人令牌
cbot_for_v2board = token
# 群组用户名【必须带有@】
group_username = @username
# 群组链接
group_url = https://t.me/xxxxxx

[V2board]
# 机场名称
name = name
# 机场官网链接
url = https://xxxxxx


